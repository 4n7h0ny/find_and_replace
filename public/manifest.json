{
  "name": "Find and replace",
  "description": "A tool that help you find and replace text.",
  "version": "2.1",
  "manifest_version": 3,
  "permissions": ["tabs", "activeTab", "scripting", "storage"],
  "action": {
    "default_popup": "index.html",
    "default_icon": "find_and_replace.png"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
